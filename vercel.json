{
  "builds": [
    {
      "src": "Backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "myan-san/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Backend/server.js"
    },
    {
      "source": "/static/(.*)",
      "destination": "/myan-san/build/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/myan-san/build/index.html"
    }
  ]
}
