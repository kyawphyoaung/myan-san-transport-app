{
  "version": 2,
  "builds": [
    {
      "src": "Backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "myan-san/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Backend/server.js"
    },
    {
      "source": "/static/(.*)",
      "destination": "/myan-san/static/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "/myan-san/favicon.ico"
    },
    {
      "source": "/manifest.json",
      "destination": "/myan-san/manifest.json"
    },
    {
      "source": "/robots.txt",
      "destination": "/myan-san/robots.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/myan-san/index.html"
    }
  ]
}